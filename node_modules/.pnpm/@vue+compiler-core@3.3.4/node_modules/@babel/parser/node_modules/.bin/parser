#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/@babel+parser@7.22.16/node_modules/@babel/parser/bin/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/@babel+parser@7.22.16/node_modules/@babel/parser/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/@babel+parser@7.22.16/node_modules/@babel/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/@babel+parser@7.22.16/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/node_modules:/mnt/e/q-designer/pnpmtest/node_modules:/mnt/e/q-designer/node_modules:/mnt/e/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/@babel+parser@7.22.16/node_modules/@babel/parser/bin/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/@babel+parser@7.22.16/node_modules/@babel/parser/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/@babel+parser@7.22.16/node_modules/@babel/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/@babel+parser@7.22.16/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/node_modules:/mnt/e/q-designer/pnpmtest/node_modules:/mnt/e/q-designer/node_modules:/mnt/e/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../bin/babel-parser.js" "$@"
fi
