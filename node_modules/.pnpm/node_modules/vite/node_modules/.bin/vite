#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vite@4.4.9/node_modules/vite/bin/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vite@4.4.9/node_modules/vite/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vite@4.4.9/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/node_modules:/mnt/e/q-designer/pnpmtest/node_modules:/mnt/e/q-designer/node_modules:/mnt/e/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vite@4.4.9/node_modules/vite/bin/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vite@4.4.9/node_modules/vite/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vite@4.4.9/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/node_modules:/mnt/e/q-designer/pnpmtest/node_modules:/mnt/e/q-designer/node_modules:/mnt/e/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
