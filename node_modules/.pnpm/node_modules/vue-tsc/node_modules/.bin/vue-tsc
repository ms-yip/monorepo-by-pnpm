#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vue-tsc@1.8.11_typescript@5.2.2/node_modules/vue-tsc/bin/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vue-tsc@1.8.11_typescript@5.2.2/node_modules/vue-tsc/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vue-tsc@1.8.11_typescript@5.2.2/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/node_modules:/mnt/e/q-designer/pnpmtest/node_modules:/mnt/e/q-designer/node_modules:/mnt/e/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vue-tsc@1.8.11_typescript@5.2.2/node_modules/vue-tsc/bin/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vue-tsc@1.8.11_typescript@5.2.2/node_modules/vue-tsc/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/vue-tsc@1.8.11_typescript@5.2.2/node_modules:/mnt/e/q-designer/pnpmtest/node_modules/.pnpm/node_modules:/mnt/e/q-designer/pnpmtest/node_modules:/mnt/e/q-designer/node_modules:/mnt/e/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../../bin/vue-tsc.js" "$@"
fi
